@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}


<div class="grid grid-cols-4 gap-4">
    @foreach (var vinyl in Model.Vinyls)
    {
        <div class="border-solid border-2">
            <h1 class="font-bold">@vinyl.Title</h1>
            <h5>@vinyl.ArtistFullName</h5>
            <p>@vinyl.Type</p>
            <p class="underline">
                @vinyl.Price €
            </p>
        </div>
    }
</div>
